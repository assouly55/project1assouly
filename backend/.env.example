# Tender AI Platform - Environment Configuration
# Copy this to .env and fill in your values

# Database (PostgreSQL)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tender_ai

# DeepSeek AI (for text processing)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Mistral AI (for OCR - Vision API)
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_API_URL=https://api.mistral.ai/v1/chat/completions
MISTRAL_OCR_MODEL=pixtral-12b-2409

# Scraper Settings
SCRAPER_HEADLESS=true
SCRAPER_MAX_CONCURRENT=5
SCRAPER_RETRY_ATTEMPTS=3

# Test Mode (run immediately instead of scheduled)
TEST_MODE=true

# Debug Mode
DEBUG=true